{
  "name": "systemconfig-omnibus",
  "version": "1.0.0",
  "author": "Systemarchitektur AG",
  "summary": "Installs the gitlab service as an omnibus installation with backup assistance",
  "tags": [
    "gitlab",
    "omnibus"
  ],
  "license": "Apache-2.0",
  "source": "https://github.com/systarch/systemconfig-omnibus",
  "project_page": "https://github.com/systarch/systemconfig-omnibus",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.25.0"
    },
    {
      "name":  "puppet-gitlab",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    { "operatingsystem": "Debian", "operatingsystemrelease": ["10"]},
    { "operatingsystem": "Ubuntu", "operatingsystemrelease": ["20.04"]}
  ]
}
